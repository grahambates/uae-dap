{
  "name": "uae-dap",
  "version": "0.0.0",
  "description": "Debug Adapter Protocol for Amiga development with FS-UAE or WinUAE",
  "main": "out/debugAdapter.js",
  "engines": {
    "node": "*"
  },
  "bin": "cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/grahambates/uae-dap"
  },
  "keywords": [
    "m68k",
    "68000",
    "assembly",
    "asm",
    "dap",
    "debug",
    "amiga",
    "commodore",
    "fs-uae",
    "win-uae"
  ],
  "scripts": {
    "check": "npm run lint && npm run tsc && npm run test",
    "build": "npm run esbuild -- --minify",
    "build-dev": "npm run esbuild -- --sourcemap",
    "watch": "npm run esbuild -- --sourcemap --watch",
    "clean": "rm -rf ./out/*",
    "esbuild": "esbuild src/debugAdapter.ts src/debugAdapterWinUAE.ts --bundle --outdir=. '--entry-names=out/[name]' --platform=node",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "tsc": "tsc -p . --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "GNU General Public License v3.0",
  "dependencies": {
    "@vscode/debugadapter": "^1.55.1",
    "@vscode/debugprotocol": "^1.55.1",
    "expression-eval": "^5.0.0",
    "glob": "^8.0.1",
    "vscode-uri": "^3.0.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "esbuild": "^0.14.39",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
