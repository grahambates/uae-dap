{
  "name": "uae-dap",
  "version": "0.0.0",
  "description": "Debug Adapter Protocol for Amiga development with FS-UAE or WinUAE",
  "main": "out/debugAdapter.js",
  "engines": {
    "node": "*"
  },
  "bin": "cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/grahambates/uae-dap"
  },
  "keywords": [
    "m68k",
    "68000",
    "assembly",
    "asm",
    "dap",
    "debug",
    "amiga",
    "commodore",
    "fs-uae",
    "win-uae"
  ],
  "scripts": {
    "check": "npm run lint && npm run type-check && npm run test",
    "build": "tsc -p . --declaration",
    "watch": "npm run build -- --watch",
    "type-check": "npm run build -- --noEmit",
    "clean": "rm -rf ./out/*",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "license": "GNU General Public License v3.0",
  "dependencies": {
    "@vscode/debugadapter": "^1.55.1",
    "@vscode/debugprotocol": "^1.55.1",
    "expression-eval": "^5.0.0",
    "glob": "^8.0.1",
    "promise-retry": "^2.0.1",
    "vscode-uri": "^3.0.3"
  },
  "devDependencies": {
    "@johanblumenberg/ts-mockito": "^1.0.32",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.31",
    "@types/promise-retry": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vscode/debugadapter-testsupport": "^1.55.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
